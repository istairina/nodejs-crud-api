{
  "name": "nodejs-crud-api",
  "version": "0.0.1",
  "description": "task for rss https://github.com/AlreadyBored/nodejs-assignments/blob/main/assignments/crud-api/assignment.md",
  "main": "dist/index.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec node --loader ts-node/esm src/server.ts",
    "build": "webpack",
    "start": "node dist/bundle.js",
    "start:dev": "nodemon --exec npx ts-node src/index.ts",
    "start:prod": "npm run build && npm run start",
    "start:multi": "nodemon --exec npx ts-node src/index-cluster.ts",
    "lint": "eslint . --ext .ts",
    "test": "jest  ./tests",
    "server": "ts-node --esm src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/istairina/nodejs-crud-api.git"
  },
  "author": "Irina Migunova (istairina)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/istairina/nodejs-crud-api/issues"
  },
  "homepage": "https://github.com/istairina/nodejs-crud-api#readme",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.3",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "uuid": "^9.0.1"
  }
}